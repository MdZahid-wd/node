            </div>

          </div>
          {{/each}}
        

        </div>
      </div>
    </div>
  
        
        <video id="video-iframe" src="/video" id="videoPlayer" width="60%" controls muted="muted" autoplay>
            <source src="../public/video/WIN_20220803_18_25_32_Pro.mp4" type="video/mkv" />
        </video>
    
  
  
