<body>
  {{>navbar loginLogoName=loginLogoName}}
  {{>slider slider=slider}}
  
    
      <section class="course-section">
        <h1 class="my-2 text-center text-secondary">english grammer</h1>
          <h6 class="text-center" >basic to advance</h6>
                <div class="card-body">
                  
                  {{!-- <div class="container service-icon text-center py-3">
                    <i class="{{this.icon}} text-secondary"></i>
                  </div> --}}
                  <div class="indexcourse-div">
                 <a class="btn btn-light" id="course-anchor" type="button" href="/courseInformation1"><span id="explore-span-id">explore</span></a>
                  </div>
                 </div>
        </section>  
        
<section id="videos-section" class="py-3">

    <div class="container">
      <div class="row mt-3">
       {{#each videos}}
        <div class="col-md-4">
          <div class="card my-3">
            <div class="card-body">

              <div class="container service-icon text-center py-3">
                <i class="{{this.icon}} text-secondary"></i>
              </div>

              <h3 class="card-title">{{this.title}}</h3>

              <p>{{this.description}}</p>

              <a href="/playVideo{{videoNo}}">watch</a>

            

            </div>
          </div>
        </div>
        {{/each}}
        
      


      </div>
    </div>
  </section>



 


    
        
   


    


    
  

